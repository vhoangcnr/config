{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "😹💪<#FF8000> Siuuu ! You got it ! What should you do next ? ...</>",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#93cdd4",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#D0666F",
          "properties": {
            "branch_icon": "",
            "fetch_status": false
          },
          "style": "plain",
          "template": "<#5FAAE8>(</>{{ .HEAD }}<#5FAAE8>)</>",
          "type": "git"
        }

      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#FF8000",
          "style": "plain",
          "template": "👍👀<#FF8000> >_ </>",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "post_prompt": "\n",
  "version": 3
}

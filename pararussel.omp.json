{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": ">_ ",
          "type": "text"
        },
        {
          "foreground": "#ffffff",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#D0666F",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": "<#5FAAE8>(</>{{ .HEAD }}<#5FAAE8>) </>",
          "type": "git"
        },
        {
          "foreground": "#DCB977",
          "style": "plain",
          "template": " \uf119 ",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#ffffff",
          "properties": {
            "command": "git log --pretty=format:%cr -1 || date +%H:%M:%S",
            "shell": "bash"
          },
          "style": "plain",
          "template": "{{ .Output }} ",
          "type": "command"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 3
}
